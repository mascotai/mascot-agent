{
  "name": "mascot-agent",
  "description": "Project starter for elizaOS",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/src/index.js",
  "module": "dist/src/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "project",
    "elizaos"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/src/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@elizaos/cli": "^1.2.12",
    "@elizaos/core": "^1.2.12",
    "@elizaos/plugin-bootstrap": "^1.2.12",
    "@elizaos/plugin-ollama": "1.2.1",
    "@elizaos/plugin-openai": "^1.0.8",
    "@elizaos/plugin-sql": "^1.2.12",
    "@elizaos/plugin-twitter": "file:./plugins/plugin-twitter-fork",
    "@mascotai/plugin-connections": "file:./plugins/plugin-connections",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@types/lru-cache": "^7.10.10",
    "prettier": "3.5.3",
    "tsup": "8.5.0",
    "typescript": "^5.6.3"
  },
  "scripts": {
    "start": "elizaos start",
    "dev": "elizaos dev",
    "build": "tsc --noEmit && tsup",
    "lint": "prettier --write ./src",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "test": "bun test",
    "format": "prettier --write ./src",
    "format:check": "prettier --check ./src",
    "check-all": "bun run type-check && bun run format:check && bun run test",
    "db:setup": "bun run src/scripts/db-setup.ts",
    "db:setup:force": "TWITTER_AUTH_DB_SETUP_COMPLETE=false bun run src/scripts/db-setup.ts"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "b165ad83e5f7a21bc1edbd83374ca087e3cd6b33"
}
