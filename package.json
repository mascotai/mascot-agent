{
  "name": "mascot-agent",
  "description": "MascotAgent - ElizaOS project with Azure integration and custom authentication",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/src/index.js",
  "module": "dist/src/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "elizaos",
    "agent",
    "ai",
    "azure",
    "twitter",
    "authentication",
    "mascot"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/src/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@elizaos/cli": "latest",
    "@elizaos/core": "latest",
    "@elizaos/plugin-bootstrap": "latest",
    "@elizaos/plugin-openai": "latest",
    "@elizaos/plugin-sql": "latest",
    "@elizaos/plugin-twitter": "latest",
    "plugin-connections": "file:./plugins/plugin-connections",
    "pg": "^8.12.0",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@types/lru-cache": "^7.10.10",
    "@types/pg": "^8.11.10",
    "prettier": "3.5.3",
    "tsup": "8.5.0",
    "typescript": "^5.6.3"
  },
  "scripts": {
    "start": "elizaos start --port 3000",
    "start:prod": "elizaos start --character characters/production.json --port 3000",
    "start:local": "elizaos start --character characters/local.json --port 3000",
    "dev": "elizaos dev --port 3000",
    "build": "tsc -p tsconfig.build.json --noEmit && tsup --config tsup.config.ts",
    "lint": "prettier --write ./src",
    "type-check": "tsc --noEmit",
    "test": "elizaos test",
    "docker:build": "docker build -t mascot-agent:latest .",
    "docker:up": "docker-compose up -d",
    "docker:up:prod": "docker-compose -f docker-compose.prod.yml up -d",
    "docker:down": "docker-compose down",
    "docker:down:prod": "docker-compose -f docker-compose.prod.yml down",
    "docker:logs": "docker-compose logs -f",
    "docker:logs:prod": "docker-compose -f docker-compose.prod.yml logs -f"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "b165ad83e5f7a21bc1edbd83374ca087e3cd6b33"
}
